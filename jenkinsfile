pipeline {
  agent any

  stages {
    stage('SonarQube Analysis') {
      steps {
        def scannerHome = tool 'sonarqube';
    withSonarQubeEnv() {
      sh '/var/lib/jenkins/tools/hudson.plugins.sonar.SonarRunnerInstallation/sonarqube/bin/sonar-scanner -Dsonar.projectKey=sema-webiste-scan'
    }
      }
    }
  }
}
